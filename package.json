{
  "name": "scully-plugins",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "scully",
    "scully:serve": "scully serve",
    "build:sitemap": "ng build scully-plugin-sitemap --prod",
    "publish:sitemap": "npm run build:sitemap && npm publish dist/scully-plugin-sitemap --access public",
    "build:regex": "ng build scully-plugin-regex --prod",
    "publish:regex": "npm run build:regex && npm publish dist/scully-plugin-regex --access public",
    "build:http404": "ng build scully-plugin-http404 --prod",
    "publish:http404": "npm run build:http404 && npm publish dist/scully-plugin-http404 --access public",
    "build:shopify": "ng build scully-plugin-shopify --prod",
    "publish:shopify": "npm run build:shopify && npm publish dist/scully-plugin-shopify --access public",
    "build:all": "npm run build:sitemap && npm run build:regex && npm run build:http404 && npm run build:shopify",
    "push": "git push -u origin master"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.19",
    "@angular/cli": "^11.2.19",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@types/dotenv": "^8.2.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.19.8",
    "codelyzer": "^6.0.0",
    "dotenv": "^8.2.0",
    "fast-xml-parser": "^3.17.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.2.4",
    "node-fetch": "^2.6.1",
    "protractor": "^7.0.0",
    "shopify-buy": "^2.11.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.6"
  }
}
