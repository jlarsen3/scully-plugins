{
  "name": "scully-plugins",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "scully": "npx scully --",
    "scully:serve": "npx scully serve --",
    "build:sitemap": "ng build scully-plugin-sitemap --configuration production",
    "publish:sitemap": "npm run build:sitemap && npm publish dist/scully-plugin-sitemap --access public",
    "build:regex": "ng build scully-plugin-regex --configuration production",
    "publish:regex": "npm run build:regex && npm publish dist/scully-plugin-regex --access public",
    "build:http404": "ng build scully-plugin-http404 --configuration production",
    "publish:http404": "npm run build:http404 && npm publish dist/scully-plugin-http404 --access public",
    "build:shopify": "ng build scully-plugin-shopify --configuration production",
    "publish:shopify": "npm run build:shopify && npm publish dist/scully-plugin-shopify --access public",
    "build:all": "npm run build:sitemap && npm run build:regex && npm run build:http404 && npm run build:shopify",
    "push": "git push -u origin master"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.16",
    "@angular/common": "^12.2.16",
    "@angular/compiler": "^12.2.16",
    "@angular/core": "^12.2.16",
    "@angular/forms": "^12.2.16",
    "@angular/platform-browser": "^12.2.16",
    "@angular/platform-browser-dynamic": "^12.2.16",
    "@angular/router": "^12.2.16",
    "@scullyio/init": "^2.1.41",
    "@scullyio/ng-lib": "^2.1.0",
    "@scullyio/scully": "^2.1.0",
    "@scullyio/scully-plugin-puppeteer": "^2.1.0",
    "rxjs": "^7.5.7",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.18",
    "@angular/cli": "^12.2.18",
    "@angular/compiler-cli": "^12.2.16",
    "@angular/language-service": "^12.2.16",
    "@types/dotenv": "^8.2.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.19.8",
    "codelyzer": "^6.0.0",
    "dotenv": "^8.2.0",
    "fast-xml-parser": "^3.17.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.7",
    "node-fetch": "^2.6.1",
    "protractor": "^7.0.0",
    "shopify-buy": "^2.11.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
